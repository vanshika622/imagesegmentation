

  {
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0.0",
    "description": "This is my extension.",
    
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [{
      "matches": ["*://*/*","<all_urls>"],
      "js": ["setpassword.js"]
    }],
    "permissions": ["storage", "activeTab",  "https://example.com/", "declarativeContent"],
    
    "browser_action": {
      "default_icon": {
        
      },
    "default_popup":"login.html"
    }
  }
  